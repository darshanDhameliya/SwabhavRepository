package ocp.violation.test;

import javax.print.attribute.standard.PrinterInfo;

import ocp.violation.FestivalType;
import ocp.violation.FixedDeposit;

public class FixedDepositTest {
	public static void main(String args[]) {
		FestivalType festival = null;
		FixedDeposit fixedDeposit=new FixedDeposit("120000234000", "darshan", 2000, 1.2f, festival.HOLI);
		
		printInfo(fixedDeposit);
	}
	public static void printInfo(FixedDeposit fixedDeposit)
	{
		System.out.println("Acc No : "+fixedDeposit.getAccNo());
		System.out.println("Name : "+fixedDeposit.getName());
		System.out.println("simple intrast : "+fixedDeposit.calculateSimpleIntrast());
	}
}
