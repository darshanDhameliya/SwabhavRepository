package com.techlab.data.parse;

import java.util.HashSet;

public class DataParse {
	private static HashSet<String> strings = new HashSet<String>();
	private static String[] data;
	


	public DataParse(HashSet<String> strings) {
		this.strings = strings;
	}

	public void dataStore() {

		Integer id;
		String name;
		String designation;
		Integer managerId;
		String dateOfJoin;
		Integer salary;
		Integer commision;
		Integer department;
		for (String string : strings) {
			data = null;
			data = string.split(",");
			
			id=Integer.parseInt(data[0]);
			name=data[1];
			designation=data[2];
			managerId=Integer.parseInt(data[3]);
			dateOfJoin=data[4];
			salary=Integer.parseInt(data[5]);
			commision=Integer.parseInt(data[6]);
			department=Integer.parseInt(data[7]);
			
		}
	}

}
