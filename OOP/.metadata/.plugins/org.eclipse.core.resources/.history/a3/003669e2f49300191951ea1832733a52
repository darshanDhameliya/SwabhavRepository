package com.techlab.calculation;

public class Calculation {

	public static int findMaxNumber(int[] arr) {
		int max=arr[0];
		for (int value : arr) {
			if (max<value)
				max=value;
		}
		return max;
	}
	public static int cube(int n) throws TestException
	{
		checkNegativeValue(n);
		return n*n*n;
	}
	public static void checkNegativeValue(int value) throws TestException
	{
		if (value<0)
			throw new TestException("value is negative");
	}
}
