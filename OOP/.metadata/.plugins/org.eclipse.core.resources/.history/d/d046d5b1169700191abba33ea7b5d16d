package com.techlab.singleton.test;

import com.techlab.singleton.DataService;

public class SingletonTest {
	public static void main(String args[]) {
		DataService singleton = DataService.getSingleton();
		System.out.println("singleton : "+singleton.hashCode());
		
		DataService newSingleton = DataService.getSingleton();
		System.out.println("new singleton : "+newSingleton.hashCode());
	}
}
