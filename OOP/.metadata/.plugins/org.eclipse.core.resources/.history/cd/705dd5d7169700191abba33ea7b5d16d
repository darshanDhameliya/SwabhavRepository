package com.techlab.singleton.test;

import com.techlab.singleton.DataService;

public class SingletonTest {
	public static void main(String args[]) {
		DataService sv1 = DataService.getSingleton();
		System.out.println("singleton : " + sv1.hashCode());

		DataService sv2 = DataService.getSingleton();
		System.out.println("new singleton : " + sv2.hashCode());
	}
}
