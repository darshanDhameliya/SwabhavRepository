package com.techlab.employee.test;

import java.util.HashSet;

import com.techlab.analyze.Analyze;
import com.techlab.data.loaded.DiskLoaded;
import com.techlab.data.loaded.URLLoaded;
import com.techlab.employee.Employee;

public class EmployeeTest {

	public static void main(String args[]) {
		Employee employee;
		Analyze analyze = null;
		
		URLLoaded urlLoaded=new URLLoaded();
		urlLoaded.dataLoaded("https://swabhav-tech.firebaseapp.com/emp.txt ");
		
		employee=analyze.maximumSalaryEmployee();
		
	}
	public static void printInfo(Employee employee)
	{
		System.out.println("Id : "+employee.getId());
		System.out.println("Name : "+employee.getId());
		System.out.println("Designation : "+employee.getId());
		System.out.println("Manager Id : "+employee.getId());
		System.out.println("Date Of Join : "+employee.getId());
		System.out.println("Salary : "+employee.getId());
		System.out.println("Commision : "+employee.getId());
		System.out.println("Department : "+employee.getId());
	}
}
