package com.techlab.game.runner;

import static org.junit.Assert.*;

import org.junit.Test;

import com.techlab.board.Board;
import com.techlab.board.Mark;
import com.techlab.player.Player;

public class GameRunnerTest {
	private Player player1=new Player("Darshan", Mark.O);
	private Player player2=new Player("Vivek", Mark.X);
	private Board board=new Board();
	@Test
	public void testSetPlayerSignInBoard() {
		GameRunner gameRunner=new GameRunner(board);
		gameRunner.setPlayerSignInBoard(player1, player2);
	}

}
