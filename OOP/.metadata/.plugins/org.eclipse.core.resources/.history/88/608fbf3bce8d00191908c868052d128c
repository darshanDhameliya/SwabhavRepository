package com.techlab.inheritance.test;

public class InheritanceTest {
	public static void main(String args[]) {
		case3();
	}

	public static void case1() {
		Man x;
		x = new Man();
		x.play();
		x.walk();
	}
	public static void case2() {
		Boy y;
		y = new Boy();
		y.play();
		y.eat();
		y.talk();
		y.walk();
	}
	public static void case3() {
		Man x;
		x=new Boy();
		x.play();
		x.walk();
	}
}
