package com.techlab.analyze;

import java.util.ArrayList;

import com.techlab.data.loaded.DataLoaded;
import com.techlab.data.parse.DataParse;
import com.techlab.employee.Employee;

public class Analyze {
	private ArrayList<Employee> employees;
	private Employee maxSalEmployee;
	private DataParse dataParse;

	public Analyze(DataLoaded dataLoaded, String path) {
		dataParse=new DataParse(dataLoaded, path);
		employees=dataParse.dataStore();
	}

	public Employee maximumSalaryEmployee() {
		for (Employee employee : employees) {
			if (maxSalEmployee.equals(""))
				maxSalEmployee = employee;
			if (!maxSalEmployee.equals("")) {
				if (maxSalEmployee.getSalary() < employee.getSalary()) {
					maxSalEmployee = employee;
				}
			}
		}
		return maxSalEmployee;
	}

	public ArrayList<Employee> getEmployees() {

		return employees;
	}
}
