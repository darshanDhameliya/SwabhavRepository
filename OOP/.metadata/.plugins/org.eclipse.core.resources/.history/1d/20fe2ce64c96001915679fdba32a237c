package srp.violation;

public class Invoice {
	private String no;
	private String date;
	private double cost;
	private float discount;
	private float gst;
	private float percentageOfDiscount;
	private float percentageOfGst;
	private double totalCost;
	
	public Invoice (String no,String date,double cost,float percentageOfDiscount,float percentageOfGst)
	{
		this.no=no;
		this.date=date;
		this.cost=cost;
		this.percentageOfDiscount=percentageOfDiscount;
		this.percentageOfGst=percentageOfGst;
	}
	public float calculateGstTex()
	{
		this.gst=(float) (cost*percentageOfGst);
		return gst;
	}
	public float calculateDiscount()
	{
		this.discount=(float) (discount*percentageOfDiscount);
		return discount;
	}
	public float calculateGstTex()
	{
		this.gst=(float) (cost*percentageOfGst);
		return gst;
	}
}
