package com.techlab.board;

import java.util.HashMap;

public class Board {
	private HashMap<String, Mark> board;

	public Board() {
		board = new HashMap<String, Mark>();
	}

	public void setCellValue(String key,Mark mark)
	{
		board.put(key, mark);
	}
	public Mark getCellValue(String key)
	{
		return board.get(key);
	}
}
