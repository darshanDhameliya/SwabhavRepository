package com.techlab.calculation;

public class Calculation {

	public static int findMaxNumber(int[] arr) {
		int max=arr[0];
		for (int value : arr) {
			if (max<value)
				max=value;
		}
		return max;
	}
	public static int cube(Integer n)
	{
		return n*n*n;
	}
	public static void checkNegativeValue(int value) throws Exception
	{
		if (value<0)
			throw new Exception();
	}
}
